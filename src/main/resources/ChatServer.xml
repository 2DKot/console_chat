<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Config for Chat Server</comment>
<entry key="port">8889</entry>
<entry key="userNamePattern">^[a-z0-9_-]{3,15}$</entry>
<entry key="loginTryCount">3</entry>
<entry key="msgQueueMaxLength">101</entry>
<entry key="commandPattern">^#[a-z]+</entry>
<entry key="commandMapFileName">commandmap.xml</entry>
<entry key="userExitCommand">exit</entry>

</properties>
